{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "module": "esnext",
    "allowImportingTsExtensions": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "react": [
        "./node_modules/react"
      ],
      "react-dom": [
        "./node_modules/react-dom"
      ],
      "tslib": [
        "./node_modules/tslib"
      ]
    }
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ]
}